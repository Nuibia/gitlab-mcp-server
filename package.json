{
  "name": "gitlab-mcp-server",
  "version": "1.0.0",
  "description": "GitLab MCP服务器 - 用于管理GitLab项目的MCP服务器",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc",
    "build:watch": "yarn clean && tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "http": "tsx src/http-server.ts",
    "http:dev": "nodemon --exec tsx src/http-server.ts"
  },
  "keywords": [
    "mcp",
    "gitlab",
    "typescript",
    "model-context-protocol"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
